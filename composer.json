{
    "name": "christiancocco/swapi",
    "description": "Swapi Integration for Laravel",
    "keywords": ["swapi"],
    "homepage": "https://github.com/christiancocco/swapi",
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "christiancocco",
            "email": "info@christiancocco.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "guzzlehttp/guzzle": "^7.0",
        "phpunit/phpunit": "9.5.x-dev",
        "laravel/framework": "^8.0",
        "inertiajs/inertia-laravel": "dev-master"
    },
    "extra": {
        "laravel": {
            "providers": [
                "ChristianCocco\\Swapi\\SwapiServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "ChristianCocco\\Swapi\\": "src/",
            "ChristianCocco\\Swapi\\Tests\\": "tests/"
        }
    }
}
